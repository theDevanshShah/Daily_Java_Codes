// Commonly Used DataTypes In C#

Integer   >> int    Age = 18;
Long      >> long   Phone_Number = 9099506866;
Float     >> float  Percentage = 7.43F;
Double    >> double Temperature = 30.05D;
Boolean   >> bool   isTrue = true;
Character >> char   Gender = 'M';
String    >> string Name = "Elon Musk";


